{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "react-router",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "functions": {
    "build/server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/build/server/index.js"
    }
  ]
}
